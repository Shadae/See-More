<% @results.each do |r| %>

  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-1" align="right" height="80px" >
      <%= image_tag "instagram.png" %>
    </div>
    <div class="col-md-1">
      <%= link_to image_tag("#{r.profile_picture}", :class=> "img-thumbnail", size: "80"), "http://instagram.com/#{r.username}", target: "_blank" %>
    </div>
    <div class="col-md-7">
      <div class="author">
        <%= link_to r.username, "http://instagram.com/#{r.username}", target: "_blank" %>
      </div>
      <%= button_to "Subscribe", authors_path(author: create_instagram_author(r)), class: 'btn btn-info' %>
    </div>
  </div>
<br>
<% end %>
